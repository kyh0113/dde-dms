/* Grid */
.ui-grid-pager-row-count-picker select { height:auto !important; padding: 2px !important;}
.ui-grid-footer-cell { vertial-align:middle !important;} 
.ui-grid-cell-focus {height:100% !important;}
.ui-grid-filter-container .ui-grid-filter-button [class^='ui-grid-icon'] {top:80% !important;}

/* Grid - GridMenu Custom */
.ui-grid-icon-menu::before{content:'\c35f'}
.ui-grid-menu-button{height:26px !important;}
.ui-grid-icon-container{width:16px; text-align:left;}
.ui-grid-menu-button .ui-grid-icon-container{margin-top:5px; margin-left:0px;}
.ui-grid-header-cell-last-col .ui-grid-cell-contents, .ui-grid-header-cell-last-col .ui-grid-filter-container, .ui-grid-header-cell-last-col .ui-grid-column-menu-button, .ui-grid-header-cell-last-col + .right.ui-grid-column-resizer{margin-right:0px;}
.ui-grid-header-cell:last-child .ui-grid-column-resizer.right{border-right:0px;}

