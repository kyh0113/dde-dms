:root select { height:20px;padding:0; line-height:180%; vertical-align:middle; padding:0; border-radius:0; border:1px solid #aaa; box-sizing: border-box; }

th img, td img {vertical-align:middle;}
img, form, input, label, fieldset, span, button  * {vertical-align:middle; margin:0; padding:0;}
em, address {font-style:normal;}
div,p filedset {margin:0;padding:0;}
button {background:transparent; overflow:visible;}
