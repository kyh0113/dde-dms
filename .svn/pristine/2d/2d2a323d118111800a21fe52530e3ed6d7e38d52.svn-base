@charset "utf-8";
/* CSS Document */
* html body {overflow-x:hidden; overflow-y: auto; _overflow:hidden; _overflow-y:hidden;_overflow-x:hidden; font-size:0.75em; padding:10px 30px 30px 30px;}
*:first-child+html body {overflow-x: hidden; overflow-y: auto;}

html, body {height:100%;  overflow:auto; overflow-x:hidden; overflow-y: auto; *overflow:hidden; _overflow:hidden; _overflow-y:hidden;_overflow-x:hidden;font-family:¸¼Àº °íµñ, malgun gothic, AppleGothicNeoSD, Apple SD »êµ¹°íµñ Neo, Microsoft NeoGothic,Droid sans, Arial,Verdana, sans-serif; font-size:12px;padding:0; margin:0;}
body { }
