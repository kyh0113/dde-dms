.lme-wrap {margin:0 auto; width:794px; padding:0 0; }
.lme-header {border-top:solid 1px #000;  border-left:solid 1px #000;  border-right:solid 1px #000; box-sizing:border-box;}
.lme-wrap h1 {text-align:center;  color:#000; text-decoration:underline;}
.lme-wrap .calendar {text-align:center;  color:#000}
.lme-wrap .unit {text-align:right; padding-bottom:5px;  padding-top:5px;}

.searchBox {padding:10px 30px; border-left:solid 1px #000;  border-right:solid 1px #000; box-sizing:border-box;}
.searchBox table {    border-bottom: solid 2px #dcdcdc;  padding-bottom: 10px;}
.lme-wrap .header {padding:0 0; position:relative;  border-left:solid 1px #000;  border-right:solid 1px #000; box-sizing:border-box;}
.content {padding:10px 30px 30px 30px; border-left:solid 1px #000;  border-right:solid 1px #000; border-bottom:solid 1px #000; box-sizing:border-box;}

.btn-print {border:solid 1px #fff; color:#fff; position:absolute; right:10px; bottom:10px; padding:5px 15px; display:inline-block;}
a.btn-print, a.btn-print:link, a.btn-print:visited {color:#fff; text-decoration:none;}
a.btn-print:hover, a.btn-print:active, a.btn-print:focus {color:#fff; text-decoration:none;}


.basicTable {background:#B4B281; margin-bottom:5px; border-spacing: 1px; }
.basicTable thead th {background-color: #bdcdf9;font-size:0.85em; padding:3px 3px; color:#000;}
.basicTable thead td {font-size:0.85em; padding:3px 3px; color:#000;}
.basicTable tbody th {font-size:0.85em; background:#bdcdf9; font-weight:bold;  text-align:center; padding:2px 0; color:#000;}
.basicTable tbody th.normal  {font-weight:normal}
.basicTable tbody td {background:#fff; text-align:right; font-size:0.85em; padding:2px 5px; color:#000;}

.basicTable tbody th.sm {font-size:0.75em; font-weight:bold;}

.basicTable thead td.table_hdtd_in,
.basicTable tbody th.table_hdtd_in,
.basicTable tbody td.table_hdtd_in {background: #bdcdf9; text-align: center ; }

.basicTableGray {background:#9c9c9c; margin-bottom:5px; border-spacing: 1px;}
.basicTableGray thead th {background-color: #c7c7c7;font-size:0.85em; padding:3px 3px; color:#000;}
.basicTableGray thead td {font-size:0.85em; padding:3px 3px; color:#000;}
.basicTableGray tbody th {font-size:0.85em; background:#c7c7c7; font-weight:bold;  text-align:center; padding:2px 0; color:#000;}
.basicTableGray tbody th.normal  {font-weight:normal}
.basicTableGray tbody td {background:#fff; text-align:right; font-size:0.85em; padding:2px 5px; color:#000;}
.basicTableGray tbody td.table_hdtd_in02 { color: #333333;  background-color: #E5E5E5;}


