header, footer, section, article, nav, aside, hgroup, address, time, blockquote {display: block;}
body {width:100%;}
h1, h2, h3, h4, h5, h6 {}
h1, h2, h3, h4, h5, h6, div, p, dl, dt, dd, ul, ol, li, form, fieldset, blockquote, address, table, thead, tbody, tfoot, tr, td, th, caption, menu {margin:0; padding:0;}
ul, ol, menu {list-style:none;}
img {border:0;}


a, a:link, a:visited {color:#666; text-decoration:none;}
a:hover, a:active, a:focus {color:#333; text-decoration:none;}

fieldset {border:0;}
legend {display:none;}
input,select, textarea, form, fieldset, legend, button {outline: none;  font-family:¸¼Àº °íµñ, malgun gothic, AppleGothicNeoSD, Apple SD »êµ¹°íµñ Neo, Microsoft NeoGothic,Droid sans, Arial,Verdana, sans-serif;  font-size:12px; font-weight:normal; color:#333; vertical-align:middle; margin:0; padding:0; box-sizing: border-box;}
select * {vertical-align:middle;}
