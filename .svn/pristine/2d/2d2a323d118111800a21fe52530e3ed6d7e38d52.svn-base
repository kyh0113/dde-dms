.al {text-align:left !important;}
.ac {text-align:center !important;}
.ar {text-align:right !important;}

.avg {padding:0 !important;}
.avg .element-wrap {margin:-1px;}
.avg .element-wrap .element-name {height:21px; width:18px; background: #bdcdf9;  }
.element-wrap {display:table; width:100%; }
.element-wrap .element-name {display:table-cell; text-align:left; padding-left:5px;}
.element-wrap .element-value {width:42px; display:table-cell; text-align:right; padding-right:5px;}

.date-wrap {display:table; width:44px;}
.date-wrap .date {display:table-cell; width:50%; text-align:left;}
.date-wrap .month {display:table-cell; text-align:left;}

.noData {padding:30px 30px 30px 30px; border-left:solid 1px #000;  border-right:solid 1px #000; border-bottom:solid 1px #000; box-sizing:border-box;}
.noData>div{font-size: 1.8em;  text-align:center;   background: #bdcdf9;   font-weight:bold;    padding:40px 0px 40px 0px;    border:solid 1px #B4B281;    margin-bottom: 10px;    }