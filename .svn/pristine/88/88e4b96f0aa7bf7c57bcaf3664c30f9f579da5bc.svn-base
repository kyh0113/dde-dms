	<!-- Spring Security - 영풍 -->
	<http auto-config="true" use-expressions="true">
		<intercept-url pattern="/yp/**" access="permitAll" />
		<form-login login-page="/yp/login/login" />
		<logout
			logout-url="/yp/login/logout"
			success-handler-ref="logoutYPSuccessHandler"
			invalidate-session="false"
			delete-cookies="JSESSIONID" />
		<csrf disabled="false" />
		<headers>
			<!-- 20200320_khj Google Chrome 접근 위해 주석처리 -->
			<!-- <content-security-policy policy-directives="script-src 'self'"/> -->
			<frame-options policy="SAMEORIGIN" />
<!-- 			<xss-protection block="true" /> -->
		</headers>
		<!-- 세션이 끊겼을때 이동할 페이지 -->
		<session-management invalid-session-url="/core/login/sessionOut.do" />
		<!-- check session time out with ajax -->
		<custom-filter after="EXCEPTION_TRANSLATION_FILTER" ref="ajaxSessionTimeoutFilter" />
	</http>

	<beans:bean id="ajaxSessionTimeoutFilter" class="com.vicurus.it.core.secure.AjaxSessionTimeoutFilter">
		<beans:property name="ajaxHeader" value="AJAX" />
	</beans:bean>

	<!-- 로그아웃시 커스텀 핸들러 -->
	<beans:bean id="logoutSuccessHandler" class="com.vicurus.it.core.secure.CustomLogoutSuccessHandler"></beans:bean>
	<!-- 로그아웃시 커스텀 핸들러 - 발행회사 -->
