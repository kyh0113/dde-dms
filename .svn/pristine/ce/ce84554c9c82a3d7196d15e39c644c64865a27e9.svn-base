.grid{height:100%;}
.form-control.fit{display:inline-block;}
.form-control.mini{width:60px;}
input[disabled], input[readonly], fieldset[disabled] input{background-color:#e5e5e5;}
input.form-control{padding-left:5px;}
input.form-control[readonly], input.form-control[disabled]{background-color:#e5e5e5 !important;}
.modal-body select{width:auto;}
.modal-body h4{padding-left:10px;}
.popup_bottom{padding-top:10px; padding-bottom:10px;}

.file-table{
	display : flex;
	flex-flow : column;
	border:1px solid #d3d3d3;
	width:100%;
	height:95%;
}
.file-table thead{
	display : table;
	table-layout : fixed;
	flex : 0 0 auto;
/* 	width : calc(100% - 0.9em); */
}
.file-table tbody{
	flex : 1 1 auto;
	display:block;
/* 	height : 100%; */
/* 	min-height:520px; */
/* 	max-height:520px; */
	overflow-y : auto;
}
.file-table tbody tr{
	width : 100%;
	display : table;
}

/* 20191204_khj file-table-short ADD */
.file-table-short{
	display : flex;
	flex-flow : column;
	border:1px solid #d3d3d3;
	width:100%;
}
.file-table-short thead{
	display : table;
	table-layout : fixed;
	flex : 0 0 auto;
/* 	width : calc(100% - 0.9em); */
}
.file-table-short tbody{
	flex : 1 1 auto;
	display:block;
	height : 170px;
	min-height:170px;
	max-height:170px;
	overflow-y : auto;
}
.file-table-short tbody tr{
	width : 100%;
	display : table;
}

