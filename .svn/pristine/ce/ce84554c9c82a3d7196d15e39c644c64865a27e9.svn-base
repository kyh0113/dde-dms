.help-button {
    display: inline-block;
    height: 22px;
    width: 22px;
    line-height: 22px;
    text-align: center;
    padding: 0;
    background-color: #65BCDA;
    color: #FFF;
    font-size: 12px;
    font-weight: 700;
    cursor: default;
/*     margin-left: 4px; */
    border-radius: 100%;
    border: 2px solid #FFF;
    -webkit-box-shadow: 0 1px 0 1px rgba(0,0,0,.2);
    box-shadow: 0 1px 0 1px rgba(0,0,0,.2);
}

.help-button:hover {
    background-color: #65BCDA;
    text-shadow: none;
} 

