package com.vicurus.it.core.common.util;

import java.util.List;

/**
 * <p>
 * ListWrapper.
 * </p>
 * @author YAME
 * @version $Revision: 001  $Date: 2013-07-01 
 */
public class ListWrapper {
	@SuppressWarnings("rawtypes")
	private List list;

	@SuppressWarnings("rawtypes")
	public ListWrapper(List list) {
		this.list = list;
	}

	@SuppressWarnings("rawtypes")
	public List getList() {
		return list;
	}

	public int getSize() {
		if ( list != null) 
			return list.size();
		else
			return 0;
	}

}
