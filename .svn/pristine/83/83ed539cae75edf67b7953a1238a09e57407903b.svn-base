@charset "UTF-8";
@import url('https://fonts.googleapis.com/css2?family=Noto+Sans+KR:wght@300;400;700&display=swap');

/* common */
* {margin: 0;padding: 0;font-size: 100%;box-sizing:border-box;}

body {color:#444;line-height:1.2;letter-spacing:-0.05em; font-family: 'Noto Sans KR', Geneva,"Malgun Gothic", 'Segoe UI', Arial, Verdana, sans-serif;}

img {vertical-align: middle;border:none;}
a img {border: none; vertical-align: middle;}

a {color: #000;text-decoration:none;cursor:pointer;}
a:visited {color: #000;}
a:hover,a:focus,a:active { color:#275ad1;}
button:hover {cursor:pointer;}

li {list-style:none; display:list-item;}
button { font-family: 'Noto Sans KR', Geneva,"Malgun Gothic", 'Segoe UI', Arial, Verdana, sans-serif; white-space: nowrap;}
input, textarea, select {padding:3px; border:1px solid #ced4da;color:#495057;box-sizing:border-box;-webkit-box-sizing:border-box; font-family: 'Noto Sans KR', Geneva,"Malgun Gothic", 'Segoe UI', Arial, Verdana, sans-serif;}
input::-ms-input-placeholder {opacity:1; font-size:11px;}
input::-webkit-input-placeholder {opacity:1; font-size:11px;}
input::-moz-placeholder {opacity:1; font-size:11px;}
input[type=text] {height:32px; padding:3px; border:1px solid #dbdfe4; text-indent:10px;}
input[type=checkbox] {border:none;}
select { min-width:100px;}

label{margin-left:3px; margin-right:10px; display:inline-block;}


table {border-collapse: collapse; width:100%;}
table th { font-weight:normal;  }
table th, table td { padding:3px 10px;}

/* scrollbar */
* { /* Scrollbar : ie */
    scrollbar-arrow-color: #979ea8;
    scrollbar-3dlight-color: #dcdde1;
    scrollbar-darkshadow-color: #dcdde1;
    scrollbar-face-color: #dfe2e9;
    scrollbar-shadow-color: #f5f6fa;
    scrollbar-track-color: #f5f6fa;
}

/* Chrome, Safari용 스크롤 바 */
::-webkit-scrollbar {width: 12px; height: 12px; border: 3px solid transparent; background:transparent; }
::-webkit-scrollbar-button:start:decrement, ::-webkit-scrollbar-button:end:increment {display: block; height: 0px;}
::-webkit-scrollbar-track {background:transparent; -webkit-border-radius: 20px; border-radius:20px; }
::-webkit-scrollbar-thumb {height: 50px; width: 50px; background: #dadce0; -webkit-border-radius: 20px; border-radius: 20px; border:1px solid transparent}

/* fontsize */
h1, h2, h3, h4, h5, h6 {font-family: 'Noto Sans KR', Geneva,"Malgun Gothic", 'Segoe UI', Arial, Verdana, sans-serif; font-weight:normal;}
h1 {font-size:60px; color:#fff;}
h2 {font-size:26px; padding-bottom:10px; border-bottom:1px solid #055bab; color:#000; margin-bottom:20px;}
h3 {font-size:32px; color:#fff; font-weight:100;}

h1 span { font-size:16px; }
h2 .loc { float:right; font-size:13px; padding-top:10px;}
h2 .loc li { display:inline-block; padding-right:20px; background:url('../images/ic_loc_arrow.png') right 50% no-repeat; vertical-align: middle;}
h2 .loc li:last-child { background:none; padding-right:0px;}

/* input */
input:hover, textarea:hover {border:1px solid #0070f1;}
input:focus, textarea:focus {outline:none; border:1px solid #0070f1; }
input::placeholder, textarea::placeholder {font-size:14px; font-family: 'Noto Sans KR', Geneva,"Malgun Gothic", 'Segoe UI', Arial, Verdana, sans-serif;font-weight:300; color:#495057;}
input[disabled]:hover, input[readonly]:hover  { border:1px solid #ced4da; }

.calendar { width:140px; background:#fff url('../images/ic_calendar.png') 95% 50% no-repeat;}
/* space */
.space10 { margin-top:10px;}
.space20 { margin-top:20px;}
.space40 { margin-top:40px;}

/* layout */
#wrapper {width:100%;}
#container {max-width:1500px; min-width:1280px; margin:0 auto; }
#contents { min-width:1500px; width:100%; padding:0px 20px;}
#sub_contents { margin-left:120px; min-width:1280px; padding:0 20px 30px 0;}
.gnb { width:140px;  text-align:center; padding:30px 20px 20px 10px; }
.gnb>ul>li { margin-bottom:10px; padding:10px 0;}
.gnb>ul>li:hover .gnb_2lv { display:block;}
.gnb a { font-size:16px; color:#fff;}
.gnb>ul>li:hover, .gnb>ul>li:focus { background:#10a2ee; border-radius:5px; }
.gnb a p { margin-top:8px;}


.sub_contents_box {  height:calc(100vh - 130px); border-radius:20px 0 0 20px; background:#fff; border:2px solid #055bab; padding:30px 50px; margin:20px 0px 0 20px; overflow-y:auto;}

.gnb, #gnb_nav {height:calc(100vh - 130px); position:fixed; top:80px; left:0px; background:#055bab;border-radius:0 20px 20px 0px; overflow-y:auto;}
#gnb_nav { visibility: hidden;  position:fixed;width:200px; padding:10px 10px 20px 10px;}
#gnb_nav a { color:#fff;}
#gnb_nav>ul>li:first-child { border-bottom:none; text-align:right;}
#gnb_nav>ul>li { border-bottom:1px solid #1878d2; padding:10px; text-align:left;}
#gnb_nav .gnb_2lv, #gnb_nav .gnb_3lv { padding:10px; }

#gnb_nav>ul>li { font-size:16px; padding:10px; }
.gnb_2lv { background:#0b4b87; font-size:15px;  }
.gnb_3lv { background:#073662; font-size:13px; }
.gnb_3lv li { background:url('../images/dot_wh.png') 0 50% no-repeat; padding-left:10px; }

.gnb_2lv li { padding:5px;}
#gnb_nav ul li:hover a, #gnb_nav ul li.on a{ display:block; color:#ffee2d; }

/* login */
.login_img { height:557px; background:url('../images/login_bg.png') 0 0 no-repeat; background-size:cover; border-bottom:1px solid #f4e5d2; }
.login_wrap {position:absolute; top:50%; left:50%; transform: translate(-50%, -50%);   }
.login_box { padding:50px 200px; background:#275ad1; text-align:center; border-radius: 10px; background: -moz-linear-gradient(-45deg, #275ad1 1%, #275ad1 50%, #3262d3 50%, #3262d3 100%);
background: -webkit-linear-gradient(-45deg, #275ad1 1%,#275ad1 50%,#3262d3 50%,#3262d3 100%);
background: linear-gradient(-45deg, #275ad1 1%,#275ad1 50%,#3262d3 50%,#3262d3 100%);}
.login_box p { margin:20px 0px; font-size:30px; color:#fff;}
.login_box ul li { margin-bottom:10px; color:#fff; }
.login_box ul li input.login { border:2px solid #fff; background:transparent; color:#fff; height:60px; width:400px;  }
.login_box ul li input.login::placeholder { color:#fff; }
.login_box ul li.rt { text-align:right;}
.btn_login {  background-color: #0d2e7e;  border: none;color: white; width:400px;line-height:60px; text-align: center; text-decoration: none; font-size: 20px;}

.login_box .group li { display:inline-block; width:calc(100% / 3); border:1px solid #fff; margin-right:-3px;padding:10px;  font-size:18px;}
.login_box .group li:hover,  .login_box .group li.on { background:#f48c05; cursor:pointer;}
.copyright { margin-top:10px; text-align:center; color:#afafaf;}

/* main */
.main_img { height:250px; background:url('../images/main_bg.png') 0 0 no-repeat; background-size:cover; border-bottom:1px solid #f4e5d2; }

#sub_contents .logo, #sub_contents .top_ic { margin-top:20px; margin-right:20px;}
.logo, .top_ic { display: inline-block; margin-top:30px; vertical-align: top;}
.logo a { font-size:24px; vertical-align:middle;  margin-left:20px;}
.logo a:hover { color:#000;}
.top_ic { float:right; padding-top:20px;}
.top_ic li:hover .topnav_my { display:block;}
.topnav_my { display:none; padding:10px 20px; position:absolute; border:1px solid #d5d5d5; background:#fff; box-shadow:3px 5px 7px 0px rgba(0,0,0,0.2); z-index:1; }
.topnav_my>ul>li { margin-bottom:5px;}

.motor { font-family: 'Segoe UI', "Malgun Gothic", Arial; margin:20px auto; text-align:center; font-size:40px; color:#797979; font-style: italic; font-weight:500;}
.motor span { color:#4590b9; }

/* contents */
.leftCon, .rightCon, .notice, .branch { float:left; vertical-align: top; }
.rightCon {width:calc(100% - 386px);}

.m_stats { width:346px; margin-right:40px; }
.m_stats_item { padding:40px 20px 60px 40px; box-shadow: 4px 4px 5px #cdcdcd;}
.m_stats_date { font-size:16px; color:#fff; font-weight:100; margin:10px 0 40px 0;}
.arrow_rt { transform: rotate(180deg); }

/* main menu */

.m_menu_item { margin-bottom:30px;}
.m_menu_item li { box-sizing: border-box; display:inline-block; background:#fff; border:1px solid #dcdcdc; width:calc((100% / 3) - 1em); margin-right:20px; margin-bottom:20px;width:-webkit-calc((100% / 3) - 1em); width:-moz-calc((100% / 3) - 1em); height:208px; text-align:center; padding:20px; }
.m_menu_item p { font-size:22px; margin-top:10px; font-weight:400; white-space: nowrap;}
.m_menu_item li:hover { border:1px solid #0d2e7e; background:#e7f1f7; cursor:pointer;}
.m_menu_item li:hover p { color:#0d2e7e;}
.m_menu_item li.nomargin { margin-right:0px;}
.m_menu_item li a { display:block;}
/* notice */
.notice {  width:calc(100% - 400px); margin-right:50px; }

.list_bl li { background:url('../images/dot.png') 0 50% no-repeat; padding-left:10px; margin-bottom:10px;}
.list_bl li span.date { float:right; font-size:12px; color:#666;  margin-left:20px;}
.list_bl li span.tel { float:right; text-align:left;  width:200px; margin-left:20px; line-height: 1.5em;}


/* background color */
.bg_gray { background:#f6f6f6;} /* main */
.bg_blue { background:#2cbcff;}
.bg_orange { background:#ffa32d;}
.bg_darkblue { background:#055bab;}


/* title */
.stitle { background:url('../images/ic_subtitle.png') 0 50% no-repeat; padding-left:20px; color:#055bab; font-size:18px;  margin-bottom:10px;  }
section { margin-bottom:2em;}


.float_wrap { width:100%; display:inline-block;}
.float_wrap .fl { float:left; }
.float_wrap .fr { float:right; }

/* button */
.btn_wrap { text-align:right; }
.btn { border-radius:5px; display:inline-block; vertical-align:middle; border:1px solid #ccc; padding:4px 13px;cursor:pointer; text-align:right; background:transparent; line-height:25px; }
.btn:hover { background-color:#1585d8; color:#fff;}


.btn_save, .btn_make, .btn_search, .btn_excel, .btn_apply { padding-left:40px; background-color:#269bf3;background-position: 10% 50%; background-repeat:  no-repeat; color:#fff; }
.btn_save { background-image:url('../images/btn_ic_save.png'); }
.btn_make { background-image: url('../images/btn_ic_make.png');  }
.btn_search { background-image:url('../images/btn_ic_search.png');  }
.btn_excel { background-image:url('../images/btn_ic_excel.png');  }
.btn_apply { background-image:url('../images/btn_ic_apply.png');  }
.btn_s, .btn_g { border-radius:3px; display:inline-block; vertical-align:baseline; border:1px solid #626262;  padding:3px 10px 4px 10px; cursor:pointer; color:#fff; font-weight:100; font-size:14px;} 
.btn_s { background:#5d5d5d;  } /* sub button */
.btn_g { background:#3853d8;  } /* grid button */
.btn_s:hover { background:#333;}
.btn_g:hover { background:#0b2190;}

/* table / grid */
.tbl_box { background:#eef8fc; padding:15px; border-radius:5px;}
.tbl_box table th { text-align:right; white-space: nowrap;}

.lst { width:100%;}
.lst th { border:1px solid #000; border-bottom:1px solid #000;  padding-bottom:10px; }
.lst td { border:1px solid #ddd; text-align:center; height:40px;}
.lst th, .lst td { padding:8px 10px;}
.lst tr:hover td { background:#eefbff; cursor:pointer;}
.lst td a { color:#083dd7; }
.lst td a:hover { text-decoration: underline;}
.lst td input[type=text]  { width:75%;  }
.lst td.right { text-align:right;}
.lst td.left { text-align:left;}
.lst tr.sum td {background:#fcffeb;}
.lst tr.total td {background:#fff4d3;}

.lst td .rate { display:inline-block; margin-left:20px; min-width:200px;  text-align:left;}

/* popup */
.pop_header { background:#055bab; color:#fff; font-size:18px; padding-left:20px; line-height:60px; width:100%; }
.pop_content { padding:20px;}

.require { color:red; font-size:16px;margin-left:5px;}

/* paging */
.paging { margin-top:20px; text-align:center;}
.paging a { margin-left:15px; color:#a7a7a7; font-weight:300; font-size:13px; padding:3px;}
.paging a.on { color:#000; text-decoration: underline;}
.paging a:hover {color:#000;  }
.paging .arrow {border:1px solid #ddd; padding:3px 5px;}
.paging .arrow:hover { border:1px solid #000;}