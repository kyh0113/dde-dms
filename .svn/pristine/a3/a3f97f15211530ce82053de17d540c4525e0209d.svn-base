				</when>
				<otherwise>
				AND B.dept_cd = #{dept_cd}
				</otherwise>
			</choose>
		</if>
		<if test="upper_key != null">AND A.UPPER_KEY = #{upper_key}</if>
		<if test="upper_val != null">AND A.UPPER_VALUE = #{upper_val}</if>
		ORDER BY ORDER_NO ASC
