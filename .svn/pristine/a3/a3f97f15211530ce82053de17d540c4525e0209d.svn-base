	<update id="update_zpp_ctd_cathode_master" parameterType="java.util.HashMap">
		UPDATE TBL_CATHODE_MASTER
		   SET M1_RESULT = ${M1_RESULT},
		   	   M2_RESULT = ${M2_RESULT},	
		   	   M3_RESULT = ${M3_RESULT},	
		   	   M4_RESULT = ${M4_RESULT},	
		   	   M5_RESULT = ${M5_RESULT},	
		   	   M6_RESULT = ${M6_RESULT},	
		   	   M7_RESULT = ${M7_RESULT},	
		   	   M8_RESULT = ${M8_RESULT},
		   	   TABLET_WORKER = #{TABLET_WORKER},
		   	   TABLET_REG_DATETIME = #{TABLET_REG_DATETIME}
		 WHERE JOB_INDEX = #{JOB_INDEX}
	</update>

	<select id="zpp_ctd_cathode_list" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_ctd_cathode_list */
		SELECT *
		  FROM TBL_CATHODE_MASTER
	     WHERE JOB_DATE BETWEEN #{sdate} AND #{edate}
			<if test='machine_id != null and machine_id != "A"'>AND MACHINE_NO = #{machine_id}</if>
			<if test='hour_index != null and hour_index != "A"'>AND JOB_TIME = #{hour_index}</if>
		    <if test='worker_name != null and worker_name != ""'>AND WORKER LIKE '%'||#{worker_name}||'%'</if>
	     ORDER BY REG_DATE
	</select>

	<select id="zpp_ctd_cathode_list_cnt" resultType="Double">
		/* oracle.yp_zpp.zpp_ctd_cathode_list_cnt */
		SELECT COUNT(*) AS CNT
		  FROM TBL_CATHODE_MASTER
	     WHERE JOB_DATE BETWEEN #{sdate} AND #{edate}
			<if test='machine_id != null and machine_id != "A"'>AND MACHINE_NO = #{machine_id}</if>
			<if test='hour_index != null and hour_index != "A"'>AND JOB_TIME = #{hour_index}</if>
		    <if test='worker_name != null and worker_name != ""'>AND WORKER LIKE '%'||#{worker_name}||'%'</if>
	</select>

	<insert id="insert_zpp_ctd_cathode_setup" parameterType="java.util.HashMap">
		INSERT INTO TBL_CATHODE_SETUP (
			REG_DATE,
			M1_LOT_WEIGHT,
			M1_PPM_LEVEL,
			M2_LOT_WEIGHT,
			M2_PPM_LEVEL,
			M3_LOT_WEIGHT,
			M3_PPM_LEVEL,
			M4_LOT_WEIGHT,
			M4_PPM_LEVEL,
			M5_LOT_WEIGHT,
			M5_PPM_LEVEL,
			M6_LOT_WEIGHT,
			M6_PPM_LEVEL,
			M7_LOT_WEIGHT,
			M7_PPM_LEVEL,
			M8_LOT_WEIGHT,
			M8_PPM_LEVEL
		)VALUES(
			#{REG_DATE}
			${M1_LOT_WEIGHT},
			${M1_PPM_LEVEL},
			${M2_LOT_WEIGHT},
			${M2_PPM_LEVEL},
			${M3_LOT_WEIGHT},
			${M3_PPM_LEVEL},
			${M4_LOT_WEIGHT},
			${M4_PPM_LEVEL},
			${M5_LOT_WEIGHT},
			${M5_PPM_LEVEL},
			${M6_LOT_WEIGHT},
			${M6_PPM_LEVEL},
