	<select id="zpp_ore_get_master_list_xls" parameterType="java.util.HashMap" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_ore_get_master_list_avg */
 		<![CDATA[
		SELECT 
			master.MATERIAL_ID, master.SHEET, master.SELLER_ID, master.IMPORT_DATE, master.LOT_COUNT,
			master.IG_1_SPLIT, master.IG_1_UMPIRE, IG_1_WIN, IG_1_FAIL,
            master.IG_2_SPLIT, master.IG_2_UMPIRE, IG_2_WIN, IG_2_FAIL,
			master.IG_3_SPLIT, master.IG_3_UMPIRE, IG_3_WIN, IG_3_FAIL,
			oca.ADJUST_IG_1, oca.ADJUST_IG_2, oca.ADJUST_IG_3, 
			ROUND((	SELECT NVL(AVG(YP), 0)
				FROM TBL_ORE_ANALYSIS_RESULT ar
				WHERE ar.MASTER_ID = master.MASTER_ID
				AND ar.INGREDIENT_ID = 'YPIG-0001'
				AND LOT_NO <= master.LOT_COUNT
			), 2) AS IG_1_AVG,
			ROUND((	SELECT NVL(AVG(YP), 0)
				FROM TBL_ORE_ANALYSIS_RESULT ar
				WHERE ar.MASTER_ID = master.MASTER_ID
				AND ar.INGREDIENT_ID = 'YPIG-0007'
				AND LOT_NO <= master.LOT_COUNT
			), 2) AS IG_2_AVG,
			ROUND((	SELECT NVL(AVG(YP), 0)
				FROM TBL_ORE_ANALYSIS_RESULT ar
				WHERE ar.MASTER_ID = master.MASTER_ID
				AND ar.INGREDIENT_ID = 'YPIG-0031'
				AND LOT_NO <= master.LOT_COUNT
			), 2) AS IG_3_AVG
		]]>
		FROM TBL_ORE_ANALYSIS_MASTER master, TBL_ORE_COMPONENT_ANALYSIS oca
	   WHERE IMPORT_DATE BETWEEN #{sdate} AND #{edate}
	     AND master.MASTER_ID = oca.MASTER_ID
		   	<if test='ore_id != null and ore_id != "ALL"'>AND MATERIAL_ID = #{ore_id}</if>
			<if test='seller_id != null and seller_id != "ALL"'>AND SELLER_ID = #{seller_id}</if>
			<if test='sheet_id != null and sheet_id != "ALL"'>AND SHEET = #{sheet_id}</if>
		ORDER BY IMPORT_DATE
	</select>

	<select id="zpp_ore_get_material_name" resultType="String">
		/* oracle.yp_zpp.zpp_ore_get_material_name */
		SELECT MATERIAL_NAME
		  FROM TBL_ORE_MATERIAL_INFO
	     WHERE MATERIAL_ID = #{MATERIAL_ID}
		   AND ROWNUM=1
	</select>

	<select id="zpp_ore_get_seller_name" resultType="String">
		/* oracle.yp_zpp.zpp_ore_get_seller_name */
		SELECT SELLER_NAME
		  FROM TBL_ORE_SELLER_INFO
	     WHERE SELLER_ID = #{SELLER_ID}
		   AND ROWNUM=1
	</select>

	<select id="zpp_ore_req_bl_list_xls" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_ore_req_bl_list_xls */
		SELECT am.MASTER_ID, am.MATERIAL_ID, NVL(am.SHEET, '0') SHEET, am.SELLER_ID, am.IMPORT_DATE, NVL(am.VESSEL_NAME, '') VESSEL_NAME, bi.DMT, bi.WMT, NVL(am.STATUS, 0) STATUS
		  FROM TBL_ORE_ANALYSIS_MASTER am, TBL_ORE_BL_INFO bi
	     WHERE IMPORT_DATE BETWEEN #{sdate} AND #{edate}
	       AND am.MATERIAL_ID = #{ore_id}
		   AND am.MASTER_ID = bi.MASTER_ID
			<if test='seller_id != null and seller_id != "ALL"'>AND am.SELLER_ID = #{seller_id}</if>
			<if test='sheet_id != null and sheet_id != "ALL"'>AND am.SHEET = #{sheet_id}</if>
	     ORDER BY am.IMPORT_DATE
	</select>

	<select id="zpp_ore_get_material_list" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_ore_get_material_list */
		SELECT MATERIAL_ID, MATERIAL_NAME 
		  FROM TBL_ORE_MATERIAL_INFO
		 ORDER BY MATERIAL_ID
	</select>

	<select id="zpp_ore_get_seller_list" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_ore_get_seller_list */
		SELECT SELLER_ID, SELLER_NAME 
		  FROM TBL_ORE_SELLER_INFO
		 ORDER BY SELLER_ID
	</select>

	<select id="zpp_ore_get_ingredient_list" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_ore_get_ingredient_list */
		SELECT INGREDIENT_ID, INGREDIENT_NAME 
		  FROM TBL_ORE_INGREDIENT_INFO
		 ORDER BY INGREDIENT_ID
	</select>

</mapper>