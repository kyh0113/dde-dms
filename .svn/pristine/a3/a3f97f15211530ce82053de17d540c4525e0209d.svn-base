    <select id="select_zpp_wsd_list_cnt" resultType="Double">
    	/* oracle.yp_zpp.select_zpp_wsd_list_cnt */
		SELECT COUNT(*) AS CNT
		FROM TBL_WORK_DOC
		WHERE STATUS = 'O'
	</select>

	<select id="zpp_wsd_version_list" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_wsd_version_list */
		SELECT VERSION ,GW_EDOC_STATUS
		FROM TBL_WORK_DOC_VERSION
	    WHERE CODE = #{code}
	    ORDER BY VERSION DESC
	</select>

	<select id="zpp_wsd_link" resultType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_wsd_link */
		SELECT CODE
			,VERSION
			,FILE_URL
			,FILE_NAME
			,PDF_URL
			,PDF_NAME
		FROM TBL_WORK_DOC_VERSION
	    WHERE CODE = #{code}
	    	AND VERSION = #{version}
	</select>

	<select id="wsd_edoc_write" resultType="java.util.HashMap">
		/* oracle.yp_zpp.wsd_edoc_write */
		SELECT A.CODE
			,B.TYPE1
			,B.TYPE2 
			,B.DOC_TITLE
			,TO_CHAR(A.VERSION) AS VERSION
			,A.PDF_URL
			,A.PDF_NAME
			,A.REG_DATE
		FROM TBL_WORK_DOC_VERSION A
		LEFT JOIN TBL_WORK_DOC B
		  ON A.CODE = B.CODE
		WHERE A.CODE = #{CODE}
	    	AND A.VERSION = #{VERSION}
	</select>

	<update id="zpp_wsd_create_save" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_wsd_create_save */
		INSERT INTO TBL_WORK_DOC
		(	DOC_TYPE
			,TYPE1
			,TYPE2
			,CODE
			,DOC_TITLE
			,REG_DATE
			,REG_EMP
			,STATUS
		)VALUES(
			#{doc_type}
			,#{type1}
			,#{type2}
			,#{doc_no}
			,#{doc_title}
			,sysdate
			,#{emp_code}
			,'O'
		)
	</update>
	
	<update id="zpp_wsd_create_version_save" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_wsd_create_version_save */
		/*2022.12.28 kjy GW_EDOC_STATUS null 오류 수정 -> 'F' 값으로 고정 처리 */
		INSERT INTO TBL_WORK_DOC_VERSION
		(	CODE
			,VERSION
			,FILE_URL
			,FILE_NAME
			,REG_DATE
			,REG_EMP
			,PDF_URL
			,PDF_NAME
			,GW_EDOC_STATUS
		)VALUES(
			#{doc_no}
			,#{doc_ver}
			,#{file_url}
			,#{file_name}
			,sysdate
			,#{emp_code}
			,#{pdf_url}
			,#{pdf_name}
			,'F'
		)
	</update>
	
	<update  id="zpp_wsd_update_save" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_wsd_update_save */
			UPDATE TBL_WORK_DOC
			SET UPD_DATE = SYSDATE
				,UPD_EMP = #{emp_code}
			WHERE CODE = #{doc_no}
	</update>
	
	<update  id="zpp_wsd_update_status" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.zpp_wsd_update_save */
			UPDATE TBL_WORK_DOC_VERSION
			SET GW_EDOC_DATE = SYSDATE
				,GW_EDOC_STATUS = #{STATUS}
				,GW_EDOC_NO = #{EDOC_NO}
			WHERE CODE = #{CODE}
	    		AND VERSION = #{VERSION}
	</update>

