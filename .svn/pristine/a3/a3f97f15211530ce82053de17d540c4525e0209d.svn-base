<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="oracle.yp_zpp">

	<update id="merge_creatCreReg" parameterType="java.util.HashMap">
	MERGE INTO TBL_CLEAN_DETAIL A
	USING (
		SELECT
			replace(#{STD_DT},'/','') AS STD_DT,
			#{FACTORY_NM} AS FACTORY_NM,
			#{F_LINE} AS F_LINE,
		    #{F_ROW} AS F_ROW,
		    #{CLEAN_GB} AS CLEAN_GB,
		    #{CLEAN_ECELL} AS CLEAN_ECELL,
		    replace(#{CLEAN_DT},'/','') AS CLEAN_DT,
		    replace(#{BF_DT},'/','') AS BF_DT,
		    #{CALC_CLEAN} AS CALC_CLEAN
		    <!-- #{OVER_ECELL} AS OVER_ECELL, -->
		    <!-- replace(#{OVER_DT},'/','') AS OVER_DT, -->
		    <!-- #{CH_ANODE} AS CH_ANODE -->
		FROM DUAL
	) B
	ON (
		A.STD_DT = replace(B.STD_DT,'/','')
		AND	A.FACTORY_NM = B.FACTORY_NM
		AND	A.F_LINE = B.F_LINE
		AND	A.F_ROW = B.F_ROW
	)
	WHEN NOT MATCHED THEN
	INSERT (
	    STD_DT,
		FACTORY_NM,
		F_LINE,
		F_ROW,
		CLEAN_GB,
		CLEAN_ECELL,
		CLEAN_DT,
		BF_DT,
		CALC_CLEAN
		<!-- OVER_ECELL, -->
		<!-- OVER_DT, -->
		<!-- CH_ANODE -->
	) VALUES (
	    replace(B.STD_DT,'/',''),
		B.FACTORY_NM,
		B.F_LINE,
		B.F_ROW,
		B.CLEAN_GB,
		B.CLEAN_ECELL,
		replace(B.CLEAN_DT,'/',''),
		replace(B.BF_DT,'/',''),
		B.CALC_CLEAN
		<!-- B.OVER_ECELL, -->
		<!-- replace(B.OVER_DT,'/',''), -->
		<!-- B.CH_ANODE -->
	)
	WHEN MATCHED THEN
	UPDATE SET
		CLEAN_GB = B.CLEAN_GB,
		CLEAN_ECELL = B.CLEAN_ECELL,
		CLEAN_DT = replace(B.CLEAN_DT,'/',''),
		CALC_CLEAN = B.CALC_CLEAN
		<!-- OVER_ECELL = B.OVER_ECELL, -->
		<!-- OVER_DT = replace(B.OVER_DT,'/',''), -->
		<!-- CH_ANODE = B.CH_ANODE -->
	</update>

	<select id="select_zpp_cre_list" resultType="java.util.HashMap" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.select_zpp_cre_list */
		SELECT A.STD_DT
		      ,A.FACTORY_NM||'공장' FACTORY_NM
		      ,CASE WHEN 8 >= A.F_LINE THEN '#'||A.F_LINE ELSE A.F_LINE END F_LINE
		      ,CASE WHEN A.F_ROW = 'A' THEN '남' when A.F_ROW = 'B' then '북' ELSE A.F_ROW END F_ROW
		      ,CASE WHEN A.CLEAN_GB = 'A' THEN '완전청소' when A.CLEAN_GB = 'B' then '부분청소' ELSE '완전청소' END CLEAN_GB
		      ,A.CLEAN_ECELL
		      ,A.CLEAN_DT
		      <!-- ,NVL(TO_CHAR(TO_DATE(A.STD_DT), 'YYYYMMDD') - TO_CHAR(TO_DATE(A.CLEAN_DT), 'YYYYMMDD'),0) as CALC_CLEAN -->
		      ,NVL(A.BF_DT,'-') BF_DT
		      <!-- ,NVL(A.OVER_ECELL,'-') OVER_ECELL -->
		      <!-- ,NVL(A.OVER_DT,'-') OVER_DT -->
		      <!--,CASE WHEN NVL(A.OVER_ECELL,0) != '' THEN NVL(TO_CHAR(TO_DATE(A.STD_DT), 'YYYYMMDD') - TO_CHAR(TO_DATE(A.OVER_DT), 'YYYYMMDD'),0) ELSE 0 END CALC_OVER -->
		      <!-- ,A.CH_ANODE -->
		      ,NVL(A.CALC_CLEAN,0) CALC_CLEAN
		FROM TBL_CLEAN_DETAIL A
		WHERE A.STD_DT BETWEEN replace(#{sdate},'/','') AND replace(#{edate},'/','')
		<if test='ser_factory != null and ser_factory != "ALL"'>AND A.FACTORY_NM = #{ser_factory}</if>
		<if test='ser_line != null and ser_line != "ALL"'>AND A.F_LINE = #{ser_line}</if>
		ORDER BY A.STD_DT, A.FACTORY_NM, A.F_LINE, A.F_ROW ASC
	</select>

    <select id="select_zpp_cre_list_cnt" resultType="Double">
    	/* oracle.yp_zpp.select_zpp_cre_list_cnt */
		SELECT count(1)
		FROM TBL_CLEAN_DETAIL
		WHERE STD_DT BETWEEN replace(#{sdate},'/','') AND replace(#{edate},'/','')
		<if test='ser_factory != null and ser_factory != "ALL"'>AND FACTORY_NM = #{ser_factory}</if>
		<if test='ser_line != null and ser_line != "ALL"'>AND F_LINE = #{ser_line}</if>
	</select>

	<select id="select_zpp_cre_list_c" resultType="java.util.HashMap" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.select_zpp_cre_list_c */
		SELECT A.STD_DT
			  ,A.FACTORY_NM
		      ,A.F_LINE
		      ,CASE WHEN 8 >= A.F_LINE THEN '#'||A.F_LINE ELSE A.F_LINE END F_LINE1
		      ,A.F_ROW
		      ,CASE WHEN A.F_ROW = 'A' THEN '남' when A.F_ROW = 'B' then '북' ELSE A.F_ROW END F_ROW1
		      ,CLEAN_GB
		      ,CASE WHEN A.CLEAN_GB = 'A' THEN '완전청소' when A.CLEAN_GB = 'B' then '부분청소' ELSE '완전청소' END CLEAN_GB1
		      ,A.CLEAN_ECELL
		      ,A.CLEAN_DT
		      ,TO_CHAR(TO_DATE(A.CLEAN_DT), 'YYYY/MM/DD') CLEAN_DT1
		      <!-- ,NVL(TO_CHAR(TO_DATE(A.STD_DT), 'YYYYMMDD') - TO_CHAR(TO_DATE(A.CLEAN_DT), 'YYYYMMDD'),0) as CALC_CLEAN -->
		      <!-- ,NVL(A.OVER_ECELL,0) OVER_ECELL -->
		      ,NVL(A.BF_DT,'-') BF_DT
		      <!-- ,TO_CHAR(TO_DATE(A.BF_DT), 'YYYY/MM/DD') BF_DT1 -->
		      <!-- ,A.OVER_DT -->
		      <!-- ,TO_CHAR(TO_DATE(A.OVER_DT), 'YYYY/MM/DD') OVER_DT1 -->
		      <!-- ,CASE WHEN A.OVER_ECELL is not null or A.OVER_ECELL != '' or A.OVER_DT is not null or A.OVER_DT != '' THEN NVL(TO_CHAR(TO_DATE(A.STD_DT), 'YYYYMMDD') - TO_CHAR(TO_DATE(A.OVER_DT), 'YYYYMMDD'),0) ELSE 0 END CALC_OVER -->
		      <!-- ,NVL(A.CH_ANODE,0) CH_ANODE --> 
		      ,NVL(A.CALC_CLEAN,0) CALC_CLEAN
		FROM TBL_CLEAN_DETAIL A
		WHERE A.STD_DT = replace(#{date},'/','')
		ORDER BY A.STD_DT, A.FACTORY_NM, A.F_LINE, A.F_ROW ASC
	</select>

    <select id="select_zpp_cre_list_cnt_c" resultType="Double">
    	/* oracle.yp_zpp.select_zpp_cre_list_cnt_c */
		SELECT count(1)
		FROM TBL_CLEAN_DETAIL
		WHERE STD_DT= replace(#{date},'/','')
	</select>
	
    <select id="retrieveCreOkList_excel" parameterType="java.util.HashMap" resultType="java.util.LinkedHashMap">
    	/* oracle.yp_ZPP.retrieveCreOkList_excel */
		SELECT A.STD_DT as "기준일"
		      ,A.FACTORY_NM||'공장' as "공장"
		      ,CASE WHEN 8 >= A.F_LINE THEN '#'||A.F_LINE ELSE A.F_LINE END as "계열"
		      ,CASE WHEN A.F_ROW = 'A' THEN '남' when A.F_ROW = 'B' then '북' ELSE A.F_ROW END as "열"
		      ,CASE WHEN A.CLEAN_GB = 'A' THEN '완전청소' when A.CLEAN_GB = 'B' then '부분청소' ELSE '완전청소' END "청소구분"
		      ,NVL(A.CLEAN_ECELL,0) as "청소전해조"
		      ,TO_CHAR(TO_DATE(A.CLEAN_DT), 'YYYY/MM/DD') as "청소일"
		      ,TO_CHAR(TO_DATE(A.BF_DT), 'YYYY/MM/DD') as "이전청소일"
		      ,NVL(A.CALC_CLEAN,0) as "청소경과일"
		      <!-- ,NVL(TO_CHAR(TO_DATE(A.STD_DT), 'YYYYMMDD') - TO_CHAR(TO_DATE(A.CLEAN_DT), 'YYYYMMDD'),0) as "청소경과일" -->
		      <!-- ,NVL(A.OVER_ECELL,0) as "이월전해조" -->
		      <!-- ,TO_CHAR(TO_DATE(A.OVER_DT), 'YYYY/MM/DD') as "이월청소일" -->
		      <!-- ,CASE WHEN A.OVER_ECELL is not null or A.OVER_ECELL != '' or A.OVER_DT is not null or A.OVER_DT != '' THEN NVL(TO_CHAR(TO_DATE(A.STD_DT), 'YYYYMMDD') - TO_CHAR(TO_DATE(A.OVER_DT), 'YYYYMMDD'),0) ELSE 0 END "이월경과일" -->
		      <!-- ,A.CH_ANODE as "아노드교체량" -->
		FROM TBL_CLEAN_DETAIL A
		WHERE A.STD_DT BETWEEN replace(#{sdate},'/','') AND replace(#{edate},'/','')
		<if test='ser_factory != null and ser_factory != "ALL"'>AND A.FACTORY_NM = #{ser_factory}</if>
		<if test='ser_line != null and ser_line != "ALL"'>AND A.F_LINE = #{ser_line}</if>
		ORDER BY A.STD_DT, A.FACTORY_NM, A.F_LINE, A.F_ROW ASC
    </select>

    <delete id="deleteAccessControl" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.deleteAccessControl */
		DELETE FROM TBL_CLEAN_DETAIL
		WHERE STD_DT = replace(#{STD_DT},'/','')
		AND	FACTORY_NM = #{FACTORY_NM}
		AND	F_LINE = #{F_LINE}
		AND	F_ROW = #{F_ROW}
	</delete>

	<select id="select_zpp_cre_bfdt" resultType="java.util.HashMap" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.select_zpp_cre_bfdt */
		SELECT NVL(MAX(A.CLEAN_DT),'-') BF_DT
			  ,TO_CHAR(TO_DATE(MAX(A.CLEAN_DT)), 'YYYY/MM/DD') BF_DT1
			  ,TO_DATE(#{ser_cldt}, 'YYYY/MM/DD') - TO_DATE(MAX(A.CLEAN_DT), 'YYYY/MM/DD') AS CALC_CLEAN
		FROM TBL_CLEAN_DETAIL A
		WHERE A.FACTORY_NM = #{ser_factory}
		AND A.F_LINE = #{ser_line}
		AND A.F_ROW = #{ser_row}
	</select>

	<select id="select_zpp_wsd_list" resultType="java.util.HashMap" parameterType="java.util.HashMap">
		/* oracle.yp_zpp.select_zpp_wsd_list */
		SELECT A.DOC_TYPE
      		  ,A.TYPE1
			  ,A.TYPE2
			  ,A.CODE
			  ,A.DOC_TITLE
			  ,A.REG_DATE
			  ,A.REG_EMP
			  ,A.UPD_DATE
			  ,B.VERSION
			  ,A.STATUS
			  ,B.GW_EDOC_STATUS
		FROM TBL_WORK_DOC A
		LEFT JOIN (SELECT CODE ,GW_EDOC_STATUS
			             ,MAX(VERSION) AS VERSION
			       FROM TBL_WORK_DOC_VERSION
			       GROUP BY CODE, GW_EDOC_STATUS) B
		ON A.CODE = B.CODE
		WHERE A.STATUS = 'O'
		<if test="type1 != '' and type1 != null">AND A.TYPE1 = #{type1}</if>
		<if test="type2 != '' and type2 != null">AND A.TYPE2 = #{type2}</if>
	    <if test="code != '' and code != null">AND A.CODE LIKE '%'||#{code}||'%'</if>
	    <if test="title != '' and title != null">AND A.DOC_TITLE LIKE '%'||#{title}||'%'</if>
	    ORDER BY A.DOC_TYPE, A.TYPE1, A.TYPE2, A.CODE ASC
	</select>

