/* Wrapper style 
 * Wrapper = #wrapper 
 * Sidebar = #side-wrapper
 */
#wrapper {
    position: absolute;
    width: 100%;
    min-height: 100%;
}

.animating, .animating #side-wrapper {
    -webkit-transition: all .1s ease-in-out;
    -moz-transition: all .1s ease-in-out;
    transition: all .1s ease-in-out;
}

#wrapper, #wrapper.minimized.animating {
    padding-left: 210px;
}

#side-wrapper, #wrapper.minimized.animating #side-wrapper {
    padding-left: 0;
    left: 0;
}

#wrapper.minimized #side-wrapper, #wrapper.animating #side-wrapper {
    padding-left: 210px;
    left: -210px;
}

#wrapper.minimized, #wrapper.animating {
    padding-left: 0;
}

#side-wrapper {
    position: absolute;
    width: 210px;
    height: 100%;
    color: #e6e6e6;
    white-space: nowrap;
    overflow-x: hidden;
    background: #31486a;
}

/* Sidebar Toggle style */
.navbar-brand[data-toggle="sidebar"] {
	background-image:url(../img/glyphicon.png); background-position:0 0; background-repeat:no-repeat;
	position:absolute; top:7px; left:-4px;
	width:38px;
	height:38px;
}
.navbar-brand[data-toggle="sidebar"]:focus, .navbar-brand[data-toggle="sidebar"]:hover {
    background-image:url(../img/glyphicon_on.png); background-position:0 0; background-repeat:no-repeat;
}

/* Sidebar Header style */

#side-wrapper > .media .media-object {
    max-height: 42px;
}

.minimized #side-wrapper > .media {
    padding: 4px 6px;
}

.minimized #side-wrapper > .media .media-object {
    border-radius: 50%;
}

/* Sidebar Nav style */
#side-wrapper > .nav-stacked > li + li {
    margin-top: 0;
    margin-left: 0;
}

#side-wrapper > .nav.nav-pills > li > a {
    color: #fff;
    border-radius: 0;
	font-size:13px;
	
}

#side-wrapper > .nav.nav-pills > li > a:focus, #side-wrapper > .nav.nav-pills > li > a:hover, #side-wrapper > .nav .open > a, #side-wrapper > .nav .open > a:focus, #side-wrapper > .nav .open > a:hover {
    color: #8cccff;
    background-color: #253340;
	
}

/* 蹂�寃쏀븳 �깭洹몄뿉 css �쟻�슜 �쐞�빐 �꽑�깮�옄 蹂�寃�   20180906_�븯�깭�쁽  */
#side-wrapper a .caret { /* #side-wrapper > .nav.nav-pills > li.dropdown > a > .caret  湲곗〈 */ 
    position: absolute;
    top: 18px;
    right: 22px;
    transform: rotate(90deg);
}

#side-wrapper > .nav.nav-pills > li.dropdown.open {

}

#side-wrapper > .nav.nav-pills > li.dropdown.open > a > .caret {
    transform: rotate(0);
}

#side-wrapper > .nav.nav-pills > li > .dropdown-menu {
    margin: 0;
    padding: 10px 0;
    position: relative;
    float: none;
    background-color: #263a58;
    border: none;
    border-radius: 0;
    -webkit-box-shadow: none;
    box-shadow: none;
}

#side-wrapper > .nav.nav-pills > li > .dropdown-menu > li:before {
    content: " ";
    margin-left: 20px;
    display: inline-block;
    width: 3px;
    height: 3px;
    vertical-align: middle;
    background-color: #2F4050;
    border-radius: 50%;
}

